@article{ WOS:000792644400001,
Author = {Sampson, Katie and Priestley, Michael and Dodd, Alyson L. and Broglia,
   Emma and Wykes, Til and Robotham, Dan and Tyrrell, Katie and Vega, Marta
   Ortega and Byrom, Nicola C.},
Title = {Key questions: research priorities for student mental health},
