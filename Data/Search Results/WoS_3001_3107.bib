@article{ WOS:000074897000005,
Author = {Schuster, B and Rudolph, U and Forsterling, F},
Title = {What determines behavioral decisions? Comparing the role of covariation
   information and attributions},
Journal = {PERSONALITY AND SOCIAL PSYCHOLOGY BULLETIN},
Year = {1998},
Volume = {24},
Number = {8},
Pages = {838-854},
Month = {AUG},
Abstract = {This study investigated whether behavioral decisions (e.g., persistence,
   help seeking) are influenced by covariation information and if this
   influence is mediated by attributions. In Experiment I, participants
   belonging to different age groups received covariation information, and
   causal attributions as well as behavioral reaction decisions were
   assessed. Experiment 1 replicated the often-documented influence of
   covariation information on attributions. Furthermore, it revealed that
   covariation information affects behavioral decisions more strongly, and
   developmentally earlier than do attributions. In Experiment
   2,participants were provided with conflicting covariation information
   and attributions. Covariation information had a stronger impact on
   reaction decisions than attributions. Experiment 3 again provided
   participants with covariation information and assessed attributions and
   behavioral choices, as well as reaction times. Participants provided
   with covariation information needed less time to choose behavioral
   reactions than to choose attributions. We conclude that behavioral
   decisions can be directly determined by covariation information without
   attributional mediation.},
