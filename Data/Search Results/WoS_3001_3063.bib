@article{ WOS:000446710400003,
Author = {Zoljanahi, Ehda and Mohammadi-Nezhad, Mahdi and Gilde, Bahram Sadeghpour
   and Taheri, Hamid Reza},
Title = {Investigating Sport Attributional Style, Sport Self-Efficacy, and
   Creativity in Elite Team Athletes},
Journal = {IRANIAN JOURNAL OF PSYCHIATRY AND BEHAVIORAL SCIENCES},
