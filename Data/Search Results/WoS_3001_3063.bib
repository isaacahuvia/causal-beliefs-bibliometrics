@article{ WOS:000367692400002,
Author = {Luedecke, Daniel and Knesebeck, Olaf von dem and Kofahl, Christopher},
Title = {Public knowledge about dementia in Germany-results of a population
   survey},
Journal = {INTERNATIONAL JOURNAL OF PUBLIC HEALTH},
Year = {2016},
Volume = {61},
Number = {1},
Pages = {9-16},
Month = {JAN},
Abstract = {Knowledge about cause, prevention, diagnosis, treatment and life impact
   of dementia is characterized by a relatively high uncertainty. People
   with care experiences and people from higher status groups know more
   about dementia. People with more knowledge are less likely to believe
   that dementia patients have a high quality of life, but tend to be less
   skeptical about early detection of dementia.
   To increase knowledge, reduce uncertainty and modify attitudes towards
   dementia and those who are afflicted, educational programs and
   contact-based approaches should be considered.},
