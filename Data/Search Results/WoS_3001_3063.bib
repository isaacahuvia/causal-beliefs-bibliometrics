@article{ WOS:000240568800006,
Author = {Ahn, Woo-kyoung and Flanagan, Elizabeth H. and Marsh, Jessecae K. and
   Sanislow, Charles A.},
Title = {Beliefs about essences and the reality of mental disorders},
Journal = {PSYCHOLOGICAL SCIENCE},
Year = {2006},
Volume = {17},
Number = {9},
Pages = {759-766},
Month = {SEP},
Abstract = {Do people believe mental disorders are real and possess underlying
   essences? The current study found that both novices and practicing
   clinicians held weaker essentialist beliefs about mental disorders than
   about medical disorders. They were also unwilling to endorse the idea
   that mental disorders are real and natural. Furthermore, compared with
   novices, mental health clinicians were less likely to endorse the view
   that there is a shared cause underlying a mental disorder and that one
   needs to remove the cause to get rid of the mental disorder. Clinicians
   were polarized on their views about whether mental disorders are
   categorical or dimensional. These findings reflect current controversies
   about mental disorders in the field at large. Do people believe mental
   disorders are real and possess underlying essences? The current study
   found that both novices and practicing clinicians held weaker
   essentialist beliefs about mental disorders than about medical
   disorders. They were also unwilling to endorse the idea that mental
   disorders are real and natural. Furthermore, compared with novices,
   mental health clinicians were less likely to endorse the view that there
   is a shared cause underlying a mental disorder and that one needs to
   remove the cause to get rid of the mental disorder. Clinicians were
   polarized on their views about whether mental disorders are categorical
   or dimensional. These findings reflect current controversies about
   mental disorders in the field at large.},
